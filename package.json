{
  "name": "jslibdemo",
  "version": "1.0.0",
  "description": "JavaScript Library Demo",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint ./src",
    "test": "nyc mocha",
    "test:watch": "mocha --watch",
    "validate": "npm-run-all --parallel lint test build",
    "prebuild": "rimraf dist",
    "build:main": "babel --copy-files --ignore *.test.js --out-dir dist src",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack -p --output-filename index.umd.min.js",
    "build": "npm-run-all --parallel build:*",
    "report-coverage": "cat ./coverage-lcov.info | codecov"
  },
  "repository": {
    "type": "git",
    "url": "git@github-hs4706:hmashaw-tech/jsLibDemo.git"
  },
  "author": "Mark Shaw",
  "license": "MIT",
  "keywords": [
    "JavaScript",
    "Demo"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "unique-random-array": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-plugin-mocha": "^5.0.0",
    "ghooks": "^2.0.2",
    "mocha": "^5.0.5",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.6.0",
    "rimraf": "^2.6.2",
    "webpack": "^3.10.0"
  },
  "nyc": {
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "reporter": [
      "text",
      "lcov"
    ],
    "include": [
      "src"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run validate"
    }
  }
}
